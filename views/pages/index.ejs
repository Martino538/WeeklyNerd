<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include('../partials/head.ejs'); %>
        <title>Martijn's API site</title>
    </head>
    <body>
        <%- include('../components/nav.ejs'); %>
        <div class="content-wrapper">
            <div class="container">
                <h1>Test</h1>
                <% dataResults.forEach(element => { %>
                    <div class="card">
                        <div class="card-header">
                            <h3><%= element.titel %></h3>
                        </div>
                        <div class="card-body">
                            <p><%= element.verslag %></p>
                        </div>
                    </div>
                <% }); %>
            </div>
        </div>
    </body>
</html>